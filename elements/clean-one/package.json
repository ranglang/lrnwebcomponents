    "elementName": "clean-one",
    "generator-wcfactory-version": "0.8.4",
    "useHAX": false,
    "useSass": false,
    "files": {
      "css": "src/clean-one.css",
      "scss": "src/clean-one.scss",
      "html": "src/clean-one.html",
      "js": "src/clean-one.js",
      "properties": "src/clean-one-properties.json",
      "hax": "src/clean-one-hax.json"
    },
    "sharedHaxProps": [],
    "sharedProps": [],
